<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/buttonAdminAgent"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Admin Agent"
        tools:ignore="HardcodedText"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="@+id/buttonVoterAgent1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>

    <Button
        android:id="@+id/buttonVoterAgent1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Voter 1"
        tools:ignore="HardcodedText"
        app:layout_constraintTop_toTopOf="@+id/buttonAdminAgent"
        app:layout_constraintBottom_toBottomOf="@+id/buttonVoterAgent2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>

    <Button
        android:id="@+id/buttonVoterAgent2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Voter 2"
        tools:ignore="HardcodedText"
        app:layout_constraintTop_toTopOf="@+id/buttonVoterAgent1"
        app:layout_constraintBottom_toBottomOf="@+id/buttonTest"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>

    <Button
        android:id="@+id/buttonTest"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Test"
        tools:ignore="HardcodedText"
        app:layout_constraintTop_toTopOf="@+id/buttonVoterAgent2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>

</androidx.constraintlayout.widget.ConstraintLayout>